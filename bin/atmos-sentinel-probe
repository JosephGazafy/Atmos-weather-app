#!/usr/bin/env python3
import re, datetime, sys, subprocess, json, math, time
try: from thefuzz import fuzz
except: sys.exit("üõ∞Ô∏è [ATMOS] Dependency missing: pip install thefuzz")

TARGETS = ["gazafy", "mckelvy", "acevedo", "tuala", "angie", "maria", "phil", "marries", "joe", "joseph", "jojo", "garrett", "evan", "janelle", "kennedy", "sina"]
GRAVITY = 9.80665

def sense_layer():
    # Coupled Sensing: Physical (Vibro) + Digital (Regex/Fuzzy)
    try:
        res = subprocess.run(['termux-sensor', '-s', 'accelerometer', '-n', '1'], capture_output=True, text=True)
        mag = math.sqrt(sum(v**2 for v in json.loads(res.stdout)['accelerometer']['values']))
        return abs(mag - GRAVITY)
    except: return 0.0

if __name__ == "__main__":
    print("üåëüíé [ATMOS-SENTINEL] AGENTIC SENSE-REASON-ACT LOOP ACTIVE")
    dev = sense_layer()
    print(f"‚úÖ [ATMOS] SEISMIC BASELINE: {dev:.4f} m/s¬≤ | REPRESENTATIONAL STABILITY: Œ≥=0.99")
